<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="900.0" prefWidth="1600.0" styleClass="background" stylesheets="@../CSS/CardCreatingOptionsStyle.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.menus.CardOptionsScene">
    <children>
        <Button layoutX="1100" layoutY="100" mnemonicParsing="false" onAction="#cardCreatorRun" styleClass="menu-option" text="create card" />
        <Button layoutX="1100" layoutY="300" mnemonicParsing="false" onAction="#importCardRun" styleClass="menu-option" text="import card" />
        <Button layoutX="1100" layoutY="500" mnemonicParsing="false" onAction="#exportCardRun" styleClass="menu-option" text="export card" />
        <Button layoutX="100" layoutY="800" mnemonicParsing="false" onAction="#back" styleClass="back" text="back" />
      <Label fx:id="cardNameLabel" layoutX="135.0" layoutY="226.0" opacity="0.0" prefHeight="87.0" prefWidth="209.0" style="-fx-alignment: center; -fx-background-color: #061466;" styleClass="menu-item" stylesheets="@../CSS/Import.css" text="card name :">
         <font>
            <Font size="29.0" />
         </font>
      </Label>
      <TextField fx:id="cardName" layoutX="347.0" layoutY="226.0" opacity="0.0" prefHeight="89.0" prefWidth="245.0">
         <font>
            <Font size="30.0" />
         </font></TextField>
      <Button fx:id="next" layoutX="639.0" layoutY="233.0" mnemonicParsing="false" opacity="0.0" styleClass="back" stylesheets="@../CSS/Import.css" text="next" />
      <Label fx:id="message" layoutX="135.0" layoutY="413.0" prefHeight="110.0" prefWidth="681.0" style="-fx-alignment: center;" textFill="WHITE">
         <font>
            <Font name="Arial" size="31.0" />
         </font>
      </Label>
    </children>
</AnchorPane>
